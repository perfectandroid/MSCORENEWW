<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_margin="2dp"
    android:background="@color/grey1"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.cardview.widget.CardView

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="10dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="1dp"
        android:background="@color/cardview_dark_background"
        tools:ignore="MissingConstraints">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:orientation="horizontal"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="5dp"
                android:id="@+id/ll1"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginLeft="5dp"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:padding="2dp"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:background="@drawable/icn_ntc"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:textSize="14dp"
                            android:padding="1dp"
                            android:layout_marginLeft="5dp"
                            android:textColor="@color/gray"
                            android:gravity="left"
                            android:text="Notice Type" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:padding="2dp"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:background="@drawable/icn_acnt_no"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:padding="1dp"
                            android:textSize="14dp"
                            android:layout_marginLeft="5dp"
                            android:textColor="@color/gray"
                            android:gravity="left"
                            android:text="A/C Number"/>
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tvName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="3.2"
                        android:textSize="15dp"
                        android:layout_marginLeft="30dp"
                        android:gravity="left"
                        android:layout_gravity="center_vertical"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:textAlignment="gravity"
                        android:layout_marginRight="2dp" />
                    <TextView
                        android:id="@+id/tvAccno"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="3.2"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:gravity="left"
                        android:layout_gravity="center_vertical"
                        android:text=""
                        android:textColor="@color/black"
                        android:textAlignment="gravity"
                        android:layout_marginRight="5dp"
                        android:layout_marginLeft="30dp"/>

                </LinearLayout>



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginLeft="5dp"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:padding="2dp"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:background="@drawable/icn_acnt_ac"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:textSize="14dp"
                            android:padding="1dp"
                            android:layout_marginLeft="5dp"
                            android:textColor="@color/gray"
                            android:gravity="left"
                            android:text="A/C Type" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:padding="2dp"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:background="@drawable/icn_notice_date"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:padding="1dp"
                            android:textSize="14dp"
                            android:layout_marginLeft="5dp"
                            android:textColor="@color/gray"
                            android:gravity="left"
                            android:text="Notice Date"/>
                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tvAcctype"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="3.2"
                        android:textSize="15dp"
                        android:layout_marginLeft="30dp"
                        android:gravity="left"
                        android:layout_gravity="center_vertical"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:textAlignment="gravity"
                        android:layout_marginRight="2dp" />
                    <TextView
                        android:id="@+id/tvNoticeDate"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="3.2"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:gravity="left"
                        android:layout_gravity="center_vertical"
                        android:text=""
                        android:textColor="@color/black"
                        android:textAlignment="gravity"
                        android:layout_marginRight="5dp"
                        android:layout_marginLeft="30dp"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_marginLeft="5dp"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:padding="2dp"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:background="@drawable/icn_rupee"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:textSize="14dp"
                            android:padding="1dp"
                            android:layout_marginLeft="5dp"
                            android:textColor="@color/gray"
                            android:gravity="left"
                            android:text="Due Amount" />
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:visibility="invisible"
                        android:padding="2dp"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:padding="1dp"
                            android:textSize="14dp"
                            android:layout_marginLeft="5dp"
                            android:textColor="@color/blue_variant1"
                            android:gravity="left"/>
                    </LinearLayout>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tvDueAmt"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="3.2"
                        android:textSize="15dp"
                        android:layout_marginLeft="30dp"
                        android:gravity="left"
                        android:layout_gravity="center_vertical"
                        android:textColor="@color/black"
                        android:textStyle="bold"
                        android:textAlignment="gravity"
                        android:layout_marginRight="2dp" />
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="3.2"
                        android:textSize="15dp"
                        android:textStyle="bold"
                        android:gravity="left"
                        android:layout_gravity="center_vertical"
                        android:text=""
                        android:textColor="@color/black"
                        android:textAlignment="gravity"
                        android:layout_marginRight="5dp"
                        android:layout_marginLeft="30dp" />

                </LinearLayout>


            </LinearLayout>

        </LinearLayout>


    </androidx.cardview.widget.CardView>
</LinearLayout>




