<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <FrameLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center"
        android:visibility="visible"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="40dp">

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:cardCornerRadius="10dp">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
                <LinearLayout
                    android:id="@+id/llownbank"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_gravity="center"
        android:gravity="center"
        android:textStyle="bold"
        android:layout_marginTop="20dp"
        android:text="OWN BANK TRANSFER "
        android:layout_toRightOf="@+id/imgimps"
        android:textColor="@color/blue_variant"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="--------------------------"
        android:textColor="@color/gray"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/rltv_ownbank"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imgimps1"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:scaleType="fitXY"
                android:layout_alignParentLeft="true"
                android:src="@drawable/icmt_ownbank"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:gravity="center"
                android:textStyle="bold"
                android:text="Own Account"
                android:paddingLeft="20dp"
                android:layout_toRightOf="@+id/imgimps1"
                android:textColor="@color/black"/>


            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:scaleType="fitXY"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:padding="2dp"
                android:tint="@color/black"
                app:srcCompat="@drawable/arr1"/>


        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rltv_otherbank"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp">

            <ImageView
                android:id="@+id/imgneft1"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:scaleType="fitXY"
                android:layout_alignParentLeft="true"
                android:src="@drawable/icmt_otherbank"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:gravity="center"
                android:textStyle="bold"
                android:text="Other Account"
                android:paddingLeft="20dp"
                android:layout_toRightOf="@+id/imgneft1"
                android:textColor="@color/black"/>


            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:scaleType="fitXY"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:tint="@color/black"
                android:padding="2dp"
                app:srcCompat="@drawable/arr1"/>


        </RelativeLayout>


    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/llotherbank"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:textStyle="bold"
        android:layout_marginTop="20dp"
        android:text="OTHER BANK TRANSFER "
        android:layout_toRightOf="@+id/imgimps"
        android:textColor="@color/blue_variant"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="--------------------------"
        android:textColor="@color/gray"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:padding="10dp"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/rltv_imps"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imgimps"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:scaleType="fitXY"
                android:layout_alignParentLeft="true"
                android:src="@drawable/imps"/>

            <TextView
                android:id="@+id/txt_imps"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:gravity="center"
                android:textStyle="bold"
                android:text="IMPS"
                android:paddingLeft="20dp"
                android:layout_toRightOf="@+id/imgimps"
                android:textColor="@color/black"/>



            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:scaleType="fitXY"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:tint="@color/black"
                android:padding="2dp"
                app:srcCompat="@drawable/arr1"/>


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rltv_neft"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp">

            <ImageView
                android:id="@+id/imgneft"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:scaleType="fitXY"
                android:layout_alignParentLeft="true"
                android:src="@drawable/neft"/>

            <TextView
                android:id="@+id/txtneft"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:gravity="center"
                android:textStyle="bold"
                android:text="NEFT"
                android:paddingLeft="20dp"
                android:layout_toRightOf="@+id/imgneft"
                android:textColor="@color/black"/>


            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:scaleType="fitXY"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:tint="@color/black"
                android:padding="2dp"
                app:srcCompat="@drawable/arr1"/>


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rltv_rtgs"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp">

            <ImageView
                android:id="@+id/imgrtgs"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:scaleType="fitXY"
                android:layout_alignParentLeft="true"
                android:src="@drawable/rtgs"/>

            <TextView
                android:id="@+id/txtrtgs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:gravity="center"
                android:textStyle="bold"
                android:text="RTGS"
                android:paddingLeft="20dp"
                android:layout_toRightOf="@+id/imgrtgs"
                android:textColor="@color/black"/>

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:scaleType="fitXY"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:tint="@color/black"
                android:padding="2dp"
                app:srcCompat="@drawable/arr1"/>


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rltv_quickpay"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="10dp">

            <ImageView
                android:id="@+id/imgquickpay"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:scaleType="fitXY"
                android:layout_alignParentLeft="true"
                android:src="@drawable/quick_pay"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:gravity="center"
                android:textStyle="bold"
                android:text="QUICK PAY"
                android:paddingLeft="20dp"
                android:layout_toRightOf="@+id/imgquickpay"
                android:textColor="@color/black"/>



            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:scaleType="fitXY"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:tint="@color/black"
                android:padding="2dp"
                app:srcCompat="@drawable/arr1"/>


        </RelativeLayout>



    </LinearLayout>
                </LinearLayout>

</LinearLayout>
            </androidx.cardview.widget.CardView>


        </LinearLayout>



    </LinearLayout>


</RelativeLayout>